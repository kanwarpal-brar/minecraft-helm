{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "required": ["minecraft"],
  "properties": {
    "minecraft": {
      "type": "object",
      "required": ["eula"],
      "properties": {
        "eula": {
          "type": "string",
          "enum": ["TRUE"]
        },
        "version": {
          "type": "string"
        },
        "ops": {
          "type": "string"
        },
        "viewDistance": {
          "type": "string",
          "pattern": "^[0-9]+$"
        },
        "serverName": {
          "type": "string"
        },
        "maxPlayers": {
          "type": "string",
          "pattern": "^[0-9]+$"
        }
      }
    }
  }
}
